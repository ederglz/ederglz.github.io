@font-face
  font-family 'icomoon'
  src url('../fonts/icomoon.eot?qzpaw1')
  src url('../fonts/icomoon.eot?qzpaw1#iefix') format('embedded-opentype'),
        url('../fonts/icomoon.ttf?qzpaw1') format('truetype'),
        url('../fonts/icomoon.woff?qzpaw1') format('woff'),
        url('../fonts/icomoon.svg?qzpaw1#icomoon') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icomoon' !important
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-plus
  margin-left -34px
  margin-right 10px
  cursor pointer
  &:before
    content "\e900"
    color greenA
    font-size 22px