@font-face
  font-family 'icomoon'
  src url('../fonts/icomoon.eot?2allpb')
  src url('../fonts/icomoon.eot?#iefix2allpb') format('embedded-opentype'),
 url('../fonts/icomoon.woff?2allpb') format('woff'),
		url('../fonts/icomoon.ttf?2allpb') format('truetype'),
		url('../fonts/icomoon.svg?2allpb#icomoon') format('svg')
  font-weight normal
  font-style normal

[class^="icon-"], [class*=" icon-"]
  font-family 'icomoon'
  speak none
  font-style normal
  font-weight normal
  font-variant normal
  text-transform none
  line-height 1
  -webkit-font-smoothing antialiased
  -moz-osx-font-smoothing grayscale

.icon-plus
  &:before
    content "\ea0a"

.icon-twitter
  &:before
    content "\ea91"

.icon-github4
  &:before
    content "\eab4"

.icon-linkedin2
  &:before
    content "\eac9"

